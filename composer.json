{
    "name": "zicht/url-bundle",
    "description": "Url utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^5.6 || ^7",
        "zicht/util": "^1.7",
        "doctrine/orm": "^2.5",
        "doctrine/common": "^2.4",
        "symfony/config": "^2.7",
        "symfony/console": "^2.7",
        "symfony/framework-bundle": "^2.7",
        "symfony/form": "^2.7",
        "symfony/security": "^2.7",
        "symfony/validator": "^2.7",
        "twig/twig": "^1 || ^2",
        "zicht/framework-extra-bundle": "^4.1 || ^5",
        "zicht/admin-bundle": "^1.1 || ^2 || ^3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4",
        "zicht/standards-php": "*@stable"
    },
    "autoload": {
        "psr-0": { "": ["src/", "tests/"] }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "5.6.40"
        }
    }
}

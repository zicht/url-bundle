{
    "name": "zicht/url-bundle",
    "description": "Url utilities for Symfony",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Zicht online",
            "email": "info@zicht.nl"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/dbal": "^2.13.1 || ^3",
        "doctrine/common": "^2.4 || ^3.0",
        "doctrine/orm": "^2.5",
        "doctrine/persistence": "^1 || ^2",
        "symfony/config": "^5.4",
        "symfony/console": "^5.4",
        "symfony/dependency-injection": "^5.4",
        "symfony/http-foundation": "^5.4",
        "symfony/security-core": "^5.4",
        "symfony/translation-contracts": "^1 || ^2 || ^3",
        "zicht/admin-bundle": "^8.1",
        "zicht/framework-extra-bundle": "^10",
        "zicht/util": "^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3",
        "phpunit/phpunit": "^8"
    },
    "autoload": {
        "psr-4": {
            "Zicht\\Bundle\\UrlBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZichtTest\\Bundle\\UrlBundle\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "lint": "php-cs-fixer fix --dry-run --diff -vvv",
        "lint-fix": "php-cs-fixer fix",
        "test": "phpunit"
    }
}
